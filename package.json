{
  "name": "application",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "SET \"NODE_ENV=production\" & node ./server/app",
    "start:dev": "SET \"NODE_ENV=development\" SET DEBUG=myapp:* & nodemon ./server/app",
    "db:create:migration": "npx sequelize migration:generate --name ",
    "db:migrate": "npx sequelize db:migrate",
    "db:rollback": "npx sequelize db:migrate:undo",
    "db:rollback:all": "npx sequelize db:migrate:undo:all",
    "db:create:seed": "npx sequelize-cli seed:generate --name ",
    "db:seed:migrate": "npx sequelize-cli db:seed:all --debug",
    "db:seed:rollback": "npx sequelize-cli db:seed:undo",
    "db:seed:rollback:all": "npx sequelize-cli db:seed:undo:all",
    "postinstall": "npx sequelize db:migrate",
    "build:css": "npx tailwindcss -i ./public/stylesheets/tailwind.css -o ./public/dist/output.css --watch"
  },
  "dependencies": {
    "await-to-js": "^3.0.0",
    "axios": "^0.27.1",
    "bcrypt": "^5.0.1",
    "colors": "^1.4.0",
    "connect-pg-simple": "^7.0.0",
    "connect-session-sequelize": "^7.1.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "^5.0.0-beta.1",
    "express-handlebars": "^6.0.3",
    "express-session": "^1.17.2",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "joi": "^17.6.0",
    "moment": "^2.29.2",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.15",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "pg-promise": "^10.11.1",
    "sequelize": "^6.18.0",
    "socket.io": "^4.4.1",
    "tailwindcss": "^3.0.23"
  },
  "devDependencies": {
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.4.1"
  }
}
